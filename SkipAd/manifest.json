{
  "manifest_version": 3,
  "name": "Skip video",
  "version": "1.0",
  "description": "Skip video",
  "permissions": ["tabs", "activeTab", "scripting", "storage"],
  "action": {
    "default_icon": {}
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {}
}
